{
  "name": "nestjs-etcd3",
  "version": "1.0.3",
  "description": "a nestjs etcd3 module",
  "author": "zzy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kyknow/nestjs-etcd3"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.json",
    "precommit": "lint-staged",
    "prepublish:npm": "yarn run build",
    "publish:npm": "yarn publish --access public",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "6.3.1",
    "etcd3": "^0.2.13",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.2.2"
  },
  "devDependencies": {
    "@nestjs/testing": "6.3.1",
    "@types/node": "^10.7.1",
    "cz-conventional-changelog": "^2.1.0",
    "jest": "^23.6.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^23.10.5",
    "typescript": "^2.4.2"
  },
  "keywords": [
    "nestjs",
    "nest",
    "etcd",
    "etcd3",
    "nestjs-etcd3"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
